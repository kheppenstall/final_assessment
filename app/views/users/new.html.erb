<h2>Create an Account</h2>

<div class="container">
  <div class="col-md-6">
    <%= bootstrap_form_for @user do |f| %>
      <%= f.email_field :email %>
      <%= f.password_field :password %>
      <%= f.password_field :password_confirmation %>
      <%= f.submit 'Create Account' %>
    <% end %>
  </div>
</div>
